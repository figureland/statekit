{
  "name": "@figureland/statekit",
  "version": "0.0.33",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "require": "./dist/vue.js",
      "import": "./dist/vue.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "require": "./dist/react.js",
      "import": "./dist/react.js"
    },
    "./svelte": {
      "types": "./dist/svelte.d.ts",
      "require": "./dist/svelte.js",
      "import": "./dist/svelte.js"
    },
    "./typed-local-storage": {
      "types": "./dist/typed-local-storage.d.ts",
      "require": "./dist/typed-local-storage.js",
      "import": "./dist/typed-local-storage.js"
    },
    "./animated": {
      "types": "./dist/animated.d.ts",
      "require": "./dist/animated.js",
      "import": "./dist/animated.js"
    }
  },
  "description": "Primitives for managing data flow using messages, events and signals",
  "scripts": {
    "build": "bun run scripts/build.ts",
    "dev": "bun --watch scripts/dev.ts",
    "prepublishOnly": "bun run build",
    "prepare": "husky",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "release": "bunx changelogen@latest --release"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/types": "^7.24.0",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/bun": "^1.0.0",
    "@types/react": "^18.2.78",
    "bun-plugin-dts": "^0.2.1",
    "eslint": "^9.0.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "superjson": "^2.2.1",
    "svelte": "^4.2.14",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.6.0",
    "vue": "^3.4.21"
  },
  "peerDependencies": {
    "@figureland/mathkit": "latest",
    "@figureland/typekit": "latest",
    "react": ">=16.8",
    "superjson": ">=2.0.0",
    "svelte": "^4.0.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "@figureland/mathkit": {
      "optional": false
    },
    "@figureland/typekit": {
      "optional": false
    },
    "vue": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "superjson": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/figureland/statekit.git"
  },
  "author": "Figure Systems",
  "bugs": {
    "url": "https://github.com/figureland/statekit"
  },
  "homepage": "https://github.com/figureland/statekit#readme",
  "dependencies": {
    "@figureland/mathkit": "latest",
    "@figureland/typekit": "latest"
  }
}
